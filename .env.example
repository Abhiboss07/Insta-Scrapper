# Copy this file to .env and fill in your actual API keys

# Single API key (basic usage)
GOOGLE_API_KEY=your_api_key_here
GOOGLE_CX=your_search_engine_id

# Multiple API keys (recommended for higher quota - comma separated)
GOOGLE_API_KEYS=key1,key2,key3,key4,key5,key6
GOOGLE_CX_IDS=cx1,cx1,cx1,cx1,cx1,cx1

# Optional: Instagram session file for better data access
INSTALOADER_SESSION_FILE=

# Optional: Maximum followers filter (default: 100000)
MAX_FOLLOWERS=100000
